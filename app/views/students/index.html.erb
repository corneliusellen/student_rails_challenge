<% @students.each do |student| %>
  <div class="student-<%= student.id %>">
    <h2><%= link_to(student.name, student_path(student)) %></h2>
    <li><%= link_to("Edit", edit_student_path(student)) %> <br></li>
    <li><%= link_to("Delete", student_path(student), method: "delete") %></li>
  </div>
<% end  %>
<br>
<%= link_to("Create New Student", new_student_path) %>
